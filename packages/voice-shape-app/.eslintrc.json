{
  "extends": ["warp/es6", "warp/jsx"],
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "class-methods-use-this": 0,
    "template-curly-spacing" : "off",
    "indent" : "off",
    "max-len": [1, 160]
  }
}
